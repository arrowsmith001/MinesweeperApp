<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="356.0" prefWidth="423.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.arrowsmith.minesweeperapp.MinesweeperController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Minesweeper" />
  <GridPane fx:id="gridPane" alignment="CENTER" gridLinesVisible="true" VBox.vgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
  </GridPane>
   <Text fx:id="infoText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <Button fx:id="playButton" onAction="#onPlayButtonClicked" mnemonicParsing="false" text="Play Again" />

</VBox>
